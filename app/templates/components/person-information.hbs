<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>

<h2>INFORMATION booth</h2>
<div>
    <table>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Email</th>
            <th>Action</th>
        </tr>


        {{#each data as |info|}}
        <tr>
            <th>{{info.pid}}</th>
            <th>{{info.fname}}</th>
            <th>{{info.lname}}</th>
            <th>{{info.age}}</th>
            <th>{{info.email}}</th>
            <th><button type="button" {{action "editAction" info}}> EDIT </button>
            <button type="button" {{action "deleteAction" info}}> Delete </button></th>
            
        </tr>
        {{/each}}
       
    </table>
</div>